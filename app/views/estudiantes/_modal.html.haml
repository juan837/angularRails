#estudianteModal.modal.fade{"aria-hidden" => "true", "aria-labelledby" => "horarioModal", :role => "dialog", :tabindex => "-1"}
  .modal-dialog.modal-lg
    .modal-content
      .modal-header
        %button.close{"aria-label" => "Close", "data-dismiss" => "modal", :type => "button"}
          %span{"aria-hidden" => "true"} Ã—
        %h4#exampleModalLabel.modal-title Horarios de la ruta
      .modal-body
        .row
          .col-md-2
            Nombre:
          .col-md-5
            %input{type: 'text', "ng-model" => 'nuevoEstudiante.nombre'}
        .row
          .col-md-2
            Apellido:
          .col-md-5
            %input{type: 'text', "ng-model" => 'nuevoEstudiante.apellido'}
        .row
          .col-md-2
            Edad:
          .col-md-5
            %input{type: 'text', "ng-model" => 'nuevoEstudiante.edad'}
        %br
        .row
          .col-md-2.col-md-offset-4
            = button_tag 'Agregar', type: 'button', class: 'btn btn-success btn-block', 'ng-click' => 'Agregar()'
